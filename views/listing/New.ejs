<%- layout('layouts/bp.ejs') %>

<body>
    <h2>Create New Listing </h2>
    <form method="POST" action="/listing">
        <input name="listing[title]" placeholder="enter Number" type="text">
        <br>
        <textarea name="listing[description]" placeholder="input description" type="text"></textarea>
        <br>
        <input name="listing[image]" placeholder="enter image url or Link" type="text">
        <br>
        <input name="listing[price]" placeholder="Enter Price" type="number">
        <br>
        <input name="listing[location]" placeholder="enter location" type="text">
        <br>
        <input name="listing[country]" placeholder="enter Country" type="text">
        <br>
        <Button>ADD</Button>
    </form>


</body>
